--Add migration
dotnet ef migrations add Migration01 --context LocadoraNET.Persistence.Contextos.LocadoraNetcontext -o Migrations

dotnet new sln -n LocadoraNET
dotnet new classlib -n LocadoraNET.Application
dotnet new classlib -n LocadoraNET.Persistence
dotnet new classlib -n LocadoraNET.Domain

dotnet sln LocadoraNET.sln add LocadoraNET.Application
dotnet sln LocadoraNET.sln add LocadoraNET.Persistence
dotnet sln LocadoraNET.sln add LocadoraNET.Domain
dotnet sln LocadoraNET.sln add LocadoraNET.API

dotnet add LocadoraNET.API/LocadoraNET.API.csproj reference LocadoraNET.Application/LocadoraNET.Application.csproj
dotnet add LocadoraNET.Application/LocadoraNET.Application.csproj reference LocadoraNET.Domain/LocadoraNET.Domain.csproj
dotnet add LocadoraNET.Application/LocadoraNET.Application.csproj reference LocadoraNET.Persistence/LocadoraNET.Persistence.csproj
dotnet add LocadoraNET.Persistence/LocadoraNET.Persistence.csproj reference LocadoraNET.Domain/LocadoraNET.Domain.csproj